{
    "version": 2,
    "builds": [
      {
        "src": "src/frontend/html/*",
        "use": "@vercel/static"
      },
      {
        "src": "src/frontend/css/*",
        "use": "@vercel/static"
      },
      {
        "src": "src/frontend/js/*",
        "use": "@vercel/static"
      },
      {
        "src": "src/frontend/public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/frontend/html/index.html"
      }
    ]
  }
  